@page
@model PayrollCalculator.Web.Pages.PreviewCostModel
@{
    ViewData["Title"] = "Preview Cost";
}

<div class="container">
    <h2 class="mt-4 mb-3">
        @Model.Preview.EmployeeName Benefit Costs
    </h2>
    <div class="row">
        <div class="col-md-8 mb-4">
            <br />
            <div class="control-group form-group">
                <div class="controls">
                    <label asp-for="Preview.FinalCost">Final Cost: @Model.Preview.FinalCost</label>                    
                </div>
            </div>
        </div>
    </div>
</div>    
